<?xml version="1.0" encoding="utf-8" ?>
<resource xmlns="http://www.wxwidgets.org/wxxrc" version="2.5.3.0">
	<object class="wxPanel" name="IncrementalSearchConfDlg">
		<object class="wxBoxSizer">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer">
					<label>Highlight colour</label>
					<object class="sizeritem">
						<object class="wxGridSizer">
							<cols>2</cols>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT1">
									<label>Current occurrence in active editor</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="btnIncSearchConfColourFound">
									<label>...</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT2">
									<label>Additional occurrences in active editor</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="btnIncSearchConfColourHighlight">
									<label>...</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxTOP|wxLEFT|wxRIGHT|wxEXPAND</flag>
				<border>5</border>
			</object>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer">
					<label>Text control backgrounds</label>
					<object class="sizeritem">
						<object class="wxGridSizer">
							<cols>2</cols>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT3">
									<label>No occurrences in active editor</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="btnIncSearchConfNotFoundBG">
									<label>...</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT4">
									<label>Search wrapped at top or bottom</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="btnIncSearchConfWrappedBG">
									<label>...</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxTOP|wxLEFT|wxRIGHT|wxEXPAND</flag>
				<border>5</border>
			</object>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer">
					<label>Other settings</label>
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<object class="wxCheckBox" name="chkIncSearchConfCenterText">
							<label>Center-align current occurrence in active editor</label>
							<checked>1</checked>
						</object>
						<flag>wxALL|wxALIGN_LEFT</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="idIncSearchSelectOnEscape">
							<label>Leaving with ESC selects current occurrence</label>
						</object>
						<flag>wxALL|wxALIGN_LEFT</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="idIncSearchSelectOnFocus">
							<label>Auto-select content of input field when starting incremental search</label>
						</object>
						<flag>wxALL|wxALIGN_LEFT</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticBoxSizer">
							<label>Toolbar settings</label>
							<orient>wxVERTICAL</orient>
							<object class="sizeritem">
								<object class="wxGridSizer">
									<cols>2</cols>
									<rows>4</rows>
									<object class="sizeritem">
										<object class="wxBoxSizer">
											<object class="sizeritem">
												<object class="wxBitmapButton" name="ID_BITMAPBUTTON1">
													<bitmap stock_id="incremental_search/highlight" stock_client="wxART_BUTTON" />
												</object>
												<flag>wxALL|wxEXPAND</flag>
												<border>5</border>
											</object>
											<object class="sizeritem">
												<object class="wxStaticText" name="ID_STATICTEXT5">
													<label>&quot;Highlight&quot;-button</label>
												</object>
												<flag>wxALL|wxALIGN_BOTTOM</flag>
												<border>5</border>
												<option>1</option>
											</object>
										</object>
										<flag>wxALL|wxALIGN_BOTTOM</flag>
										<border>5</border>
									</object>
									<object class="sizeritem">
										<object class="wxChoice" name="idIncSearchHighlightDefault">
											<content>
												<item>off</item>
												<item>on</item>
												<item>save last state</item>
											</content>
											<selection>0</selection>
										</object>
										<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
										<border>5</border>
										<option>1</option>
									</object>
									<object class="sizeritem">
										<object class="wxBoxSizer">
											<object class="sizeritem">
												<object class="wxBitmapButton" name="ID_BITMAPBUTTON2">
													<bitmap stock_id="incremental_search/selected_only" stock_client="wxART_BUTTON" />
													<default>1</default>
												</object>
												<flag>wxALL|wxEXPAND</flag>
												<border>5</border>
											</object>
											<object class="sizeritem">
												<object class="wxStaticText" name="ID_STATICTEXT6">
													<label>&quot;Selected text&quot;-button</label>
												</object>
												<flag>wxALL|wxALIGN_BOTTOM</flag>
												<border>5</border>
												<option>1</option>
											</object>
										</object>
										<flag>wxALL|wxALIGN_BOTTOM</flag>
										<border>5</border>
									</object>
									<object class="sizeritem">
										<object class="wxChoice" name="idIncSearchSelectedDefault">
											<content>
												<item>off</item>
												<item>on</item>
												<item>save last state</item>
											</content>
											<selection>0</selection>
										</object>
										<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
										<border>5</border>
										<option>1</option>
									</object>
									<object class="sizeritem">
										<object class="wxBoxSizer">
											<object class="sizeritem">
												<object class="wxBitmapButton" name="ID_BITMAPBUTTON3">
													<bitmap stock_id="incremental_search/case" stock_client="wxART_BUTTON" />
													<default>1</default>
												</object>
												<flag>wxALL|wxEXPAND</flag>
												<border>5</border>
											</object>
											<object class="sizeritem">
												<object class="wxStaticText" name="ID_STATICTEXT7">
													<label>&quot;Match case&quot;-button</label>
												</object>
												<flag>wxALL|wxALIGN_BOTTOM</flag>
												<border>5</border>
												<option>1</option>
											</object>
										</object>
										<flag>wxALL|wxALIGN_BOTTOM</flag>
										<border>5</border>
									</object>
									<object class="sizeritem">
										<object class="wxChoice" name="idIncSearchMatchCaseDefault">
											<content>
												<item>off</item>
												<item>on</item>
												<item>save last state</item>
											</content>
											<selection>0</selection>
										</object>
										<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
										<border>5</border>
										<option>1</option>
									</object>
									<object class="sizeritem">
										<object class="wxBoxSizer">
											<object class="sizeritem">
												<object class="wxBitmapButton" name="ID_BITMAPBUTTON4">
													<bitmap stock_id="incremental_search/regex" stock_client="wxART_BUTTON" />
												</object>
												<flag>wxALL|wxEXPAND</flag>
												<border>5</border>
											</object>
											<object class="sizeritem">
												<object class="wxStaticText" name="ID_STATICTEXT8">
													<label>&quot;Regular Expression&quot;-button</label>
												</object>
												<flag>wxALL|wxALIGN_BOTTOM</flag>
												<border>5</border>
												<option>1</option>
											</object>
										</object>
										<flag>wxALL|wxALIGN_BOTTOM</flag>
										<border>5</border>
									</object>
									<object class="sizeritem">
										<object class="wxChoice" name="idIncSearchRegExDefault">
											<content>
												<item>off</item>
												<item>on</item>
												<item>save last state</item>
											</content>
											<selection>0</selection>
										</object>
										<flag>wxALL|wxALIGN_BOTTOM</flag>
										<border>5</border>
										<option>1</option>
									</object>
								</object>
								<flag>wxALL|wxEXPAND</flag>
								<border>5</border>
							</object>
							<object class="sizeritem">
								<object class="wxBoxSizer">
									<object class="sizeritem">
										<object class="wxStaticText" name="ID_STATICTEXT9">
											<label>Maximum amount of items in search history</label>
										</object>
										<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
										<border>5</border>
										<option>1</option>
									</object>
									<object class="sizeritem">
										<object class="wxSpinCtrl" name="idIncSearchComboMaxItems">
											<value>20</value>
											<min>1</min>
											<max>200</max>
											<tooltip>1 - 200</tooltip>
											<help>Maximum amount of items in search history must be between 1 and 200 (both included).&#x0A;Setting a value smaler than the actual count of elements truncates the history, if the dialog is closed.</help>
										</object>
										<flag>wxALL|wxALIGN_BOTTOM</flag>
										<border>5</border>
									</object>
								</object>
								<flag>wxALL|wxEXPAND</flag>
							</object>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
			</object>
		</object>
	</object>
</resource>
