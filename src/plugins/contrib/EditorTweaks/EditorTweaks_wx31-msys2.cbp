<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="EditorTweaks wx3.1.x - MSys2" />
		<Option pch_mode="2" />
		<Option compiler="msys2mingw-gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel31_msys2/share/codeadapt/plugins/EditorTweaks" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../../../devel31_msys2" />
				<Option object_output="../../../.objs31_msys2/plugins/contrib/EditorTweaks" />
				<Option type="3" />
				<Option compiler="msys2mingw-gcc" />
				<Option parameters="--debug-log --multiple-instance --no-check-associations -ns -nd -p debug" />
				<Option host_application="./codeadapt.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CA_RELEASE_TYPE)" />
					<Add option="$(#CA_PCH_TYPE)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="`$(WX_CONFIG) --cflags`" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="../../../include" />
				</Compiler>
				<Linker>
					<Add option="`$(WX_CONFIG) --libs`" />
					<Add library="codeadapt" />
					<Add directory="../../../devel31_msys2" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ../../../devel31_msys2/share/codeadapt/EditorTweaks.zip manifest.xml *.xrc" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_CONFIG" value="wx-config --version=3.1 --prefix=$(MINGW_PREFIX)" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-std=gnu++11" />
		</Compiler>
		<Unit filename="EditorTweaks.cpp" />
		<Unit filename="EditorTweaks.h" />
		<Unit filename="EditorTweaksConfDlg.cpp" />
		<Unit filename="EditorTweaksConfDlg.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith/EditorTweaksConfDlg.wxs" />
		<Extensions>
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/EditorTweaksConfDlg.wxs" src="EditorTweaksConfDlg.cpp" hdr="EditorTweaksConfDlg.h" xrc="EditorTweaksConfDlg.xrc" fwddecl="0" i18n="1" name="EditorTweaksConfDlg" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
