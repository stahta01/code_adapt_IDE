<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="wxSmith - wxAUI wx3.1.x - MSys2" />
		<Option pch_mode="2" />
		<Option compiler="msys2mingw-gcc" />
		<Build>
			<Target title="Windows">
				<Option platforms="Windows;" />
				<Option output="../../../devel31_msys2/share/codeadapt/plugins/wxSmithAui" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs31_msys2/plugins/contrib/wxSmithAui" />
				<Option external_deps="../../../devel31_msys2/wxsmithlib.dll;" />
				<Option type="3" />
				<Option compiler="msys2mingw-gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel31_msys2/codeadapt.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-std=gnu++11" />
					<Add option="$(#CA_RELEASE_TYPE)" />
					<Add option="$(#CA_PCH_TYPE)" />
					<Add option="-Winvalid-pch" />
					<Add option='-include &quot;sdk.h&quot;' />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="`$(WX_CONFIG) --cflags`" />
				</Compiler>
				<Linker>
					<Add option="-Wl,--enable-auto-image-base" />
					<Add option="-Wl,--add-stdcall-alias" />
					<Add option="-mthreads" />
					<Add option="`$(WX_CONFIG) --libs std,aui,propgrid`" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ../../../devel31_msys2/share/codeadapt/wxSmithAui.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
				<Environment>
					<Variable name="WX_CONFIG" value="wx-config --version=3.1 --prefix=$(MINGW_PREFIX)" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="Windows;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="../../../include" />
			<Add directory="../wxSmith" />
		</Compiler>
		<Linker>
			<Add library="codeadapt" />
			<Add library="wxsmithlib" />
			<Add library="tinyxml" />
			<Add directory="../../../devel31_msys2" />
		</Linker>
