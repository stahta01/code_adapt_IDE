<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="AutoVersioning wx3.1.x - MSys2" />
		<Option pch_mode="2" />
		<Option compiler="msys2mingw-gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel31_msys2/share/codeadapt/plugins/AutoVersioning.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs31_msys2/plugins/contrib/AutoVersioning" />
				<Option type="3" />
				<Option compiler="msys2mingw-gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel31_msys2/codeadapt.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CA_RELEASE_TYPE)" />
					<Add option="$(#CA_PCH_TYPE)" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="`$(WX_CONFIG) --cflags`" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="../../../include" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add option="`$(WX_CONFIG) --libs`" />
					<Add library="codeadapt" />
					<Add library="tinyxml" />
					<Add directory="../../../devel31_msys2" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ../../../devel31_msys2/share/codeadapt/AutoVersioning.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_CONFIG" value="wx-config --version=3.1 --prefix=$(MINGW_PREFIX)" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=gnu++11" />
		</Compiler>
		<Unit filename="AutoVersioning.cpp" />
		<Unit filename="AutoVersioning.h" />
		<Unit filename="avChangesDlg.cpp" />
		<Unit filename="avChangesDlg.h" />
		<Unit filename="avConfig.h" />
		<Unit filename="avHeader.cpp" />
		<Unit filename="avHeader.h" />
		<Unit filename="avSvnRevision.cpp" />
		<Unit filename="avSvnRevision.h" />
		<Unit filename="avVersionEditorDlg.cpp" />
		<Unit filename="avVersionEditorDlg.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<wxsmith version="1">
				<gui name="wxWidgets" src="avVersionEditorDlg.cpp" main="avVersionEditorDlg" init_handlers="necessary" language="CPP" />
				<resources>
					<wxScrollingDialog wxs="wxsmith/avVersionEditorDlg.wxs" src="avVersionEditorDlg.cpp" hdr="avVersionEditorDlg.h" fwddecl="0" i18n="1" name="avVersionEditorDlg" language="CPP" />
					<wxScrollingDialog wxs="wxsmith/avChangesDlg.wxs" src="avChangesDlg.cpp" hdr="avChangesDlg.h" fwddecl="0" i18n="1" name="avChangesDlg" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
