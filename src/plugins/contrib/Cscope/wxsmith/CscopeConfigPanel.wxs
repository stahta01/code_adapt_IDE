<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="CscopeConfigPanel">
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="bszMain" member="no">
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="flsMain" member="no">
					<cols>2</cols>
					<growablecols>1</growablecols>
					<object class="sizeritem">
						<object class="wxStaticText" name="wxID_ANY" variable="lblCscopeApp" member="no">
							<label>Cscope application:</label>
						</object>
						<flag>wxLEFT|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxBoxSizer" variable="bszCppCheckApp" member="no">
							<object class="sizeritem">
								<object class="wxTextCtrl" name="ID_TXT_CPP_CHECK_APP" variable="txtCscopeApp" member="yes">
									<value>cscope</value>
								</object>
								<flag>wxEXPAND</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BTN_CPPCHECK_APP" variable="btnCscopeApp" member="no">
									<label>...</label>
									<minsize>30,-1</minsize>
									<handler function="OnbtnCscopeAppClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxLEFT|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
							</object>
						</object>
						<flag>wxLEFT|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="wxID_ANY" variable="lblCscopeWWW" member="no">
							<label>CScope Install instruction</label>
						</object>
						<flag>wxTOP|wxLEFT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxHyperlinkCtrl" name="ID_HYC_CPP_CHECK_WWW" variable="hycCscopeWWW" member="no">
							<label>http://wiki.codeblocks.org/index.php/Cscope__plugin</label>
						</object>
						<flag>wxTOP|wxLEFT|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
