           type="warning"
           msg="1;4;2"
           file="3">
        <![CDATA[warning C4[0-9]+: (.+) (in ['"]([^'"]+)['"] and ['"][^'"]+['"])(: .*)]]>
    </RegEx>
    <RegEx name="Linker error"
           type="error"
           msg="2"
           file="1">
        <![CDATA[([][{}() \t#%$!~[:alnum:]&_:+/\.-]+)[ \t]+:[ \t]+(.*error LNK[0-9]+.*)]]>
